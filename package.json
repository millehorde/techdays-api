{
  "name": "techdays-api",
  "version": "0.0.1",
  "description": "Projet for Ynov TechDays - NESTJS, TYPESCRIPT, MYSQL - 10/03/2018",
  "license": "MIT",
  "scripts": {
    "tslint-run": "./node_modules/.bin/tslint -c ./tslint.json -p ./tsconfig.json -t verbose",
    "lint": "yarn tslint-run",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "./node_modules/.bin/chokidar './src/**/*.ts' -c \"yarn lint\" --initial --verbose",
    "start": "yarn lint && yarn start:nolinter",
    "start:nolinter": "node index.js",
    "start:watch": "./node_modules/.bin/concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn lint:watch\" \"yarn start:watch:nolinter\"",
    "start:watch:nolinter": "./node_modules/.bin/nodemon --ignore '*.test.ts' --ignore '*.e2e-test.ts'",
    "prestart:prod": "./node_modules/.bin/tsc",
    "start:prod": "node dist/main.js",
    "debug": "yarn lint && node $NODE_DEBUG_OPTION ./index.js",
    "jest-run": "jest --notify --forceExit --silent=false --config=jest.json",
    "jest-run:ut": "yarn jest-run --testRegex='/src/.*\\.(test|spec).(ts|tsx|js)$'",
    "jest-run:e2e": "yarn jest-run --runInBand --testRegex='/(src|e2e)/.*\\.(e2e-test|e2e-spec).(ts|tsx|js)$'",
    "test": "yarn jest-run:ut",
    "test:coverage": "yarn jest-run:ut --coverage",
    "test:watch": "yarn jest-run:ut --watch",
    "e2e": "yarn jest-run:e2e",
    "e2e:coverage": "yarn e2e --coverage",
    "e2e:watch": "yarn e2e --watch",
    "coverage": "yarn jest-run --runInBand --coverage --testRegex='/(src|e2e)/.*\\.(e2e-test|e2e-spec|test|spec).(ts|tsx|js)$'"
  },
  "dependencies": {
    "@nestjs/common": "^4.6.5",
    "@nestjs/core": "^4.6.5",
    "@nestjs/microservices": "^4.6.5",
    "@nestjs/swagger": "^1.1.4",
    "@nestjs/typeorm": "^2.0.0",
    "@nestjs/websockets": "^4.6.5",
    "@types/bcryptjs": "^2.4.1",
    "@types/dotenv-safe": "^4.0.1",
    "@types/i18next": "^8.4.3",
    "@types/i18next-express-middleware": "^0.0.32",
    "@types/i18next-node-fs-backend": "^0.0.29",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.104",
    "@types/passport": "^0.4.3",
    "@types/passport-jwt": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "bugsnag": "^2.3.0",
    "class-validator": "^0.8.1",
    "dotenv-safe": "^4.0.4",
    "i18next": "^10.5.0",
    "i18next-express-middleware": "^1.1.1",
    "i18next-node-fs-backend": "^1.0.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.21.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "pg": "^7.4.1",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.4.0",
    "typeorm": "^0.1.15",
    "typescript": "^2.7.2",
    "winston": "^2.4.0",
    "xlsx": "^0.12.4"
  },
  "devDependencies": {
    "@gojob/tslint-config": "^0.0.3",
    "@nestjs/testing": "^4.6.1",
    "@types/express": "^4.11.0",
    "@types/faker": "^4.1.2",
    "@types/jest": "^22.2.0",
    "@types/node": "^8.9.4",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.8",
    "chokidar-cli": "^1.2.0",
    "commitizen": "^2.9.6",
    "concurrently": "^3.5.1",
    "cz-conventional-changelog": "^2.1.0",
    "faker": "^4.1.0",
    "jest": "^22.4.2",
    "nodemon": "^1.17.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.1",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.9.0",
    "tslint-config-prettier": "^1.9.0",
    "tslint-plugin-prettier": "^1.3.0"
  }
}
